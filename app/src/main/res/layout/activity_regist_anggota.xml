<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/kode"
    tools:context=".Regist_anggota">

    <TextView
        android:id="@+id/txtJudul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="28dp"
        android:gravity="center"
        android:padding="5dp"
        android:text="REGISTRASI ANGGOTA KELOMPOK"
        android:textColor="@color/kode"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:id="@+id/linier"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="403dp"
        android:layout_height="665dp"
        android:layout_marginTop="52dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="8dp"
        app:cardPreventCornerOverlap="false"
        app:cardUseCompatPadding="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraint"
            android:layout_width="441dp"
            android:layout_height="643dp">

            <TextView
                android:id="@+id/txtNama"
                android:layout_width="386dp"
                android:layout_height="19dp"
                android:layout_marginTop="16dp"
                android:text="@string/nama"
                android:textColor="@color/colorText"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.327"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/editNama"
                style="@style/styleEditText"
                android:layout_width="360dp"
                android:layout_height="38dp"
                android:background="@drawable/button1"
                android:hint="Masukkan Nama Anggota"
                android:paddingLeft="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.197"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtNama" />

            <TextView
                android:id="@+id/txtNim"
                android:layout_width="386dp"
                android:layout_height="19dp"
                android:layout_marginTop="4dp"
                android:text="@string/NIM"
                android:textColor="@color/colorText"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.327"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editNama" />

            <EditText
                android:id="@+id/editNIM"
                style="@style/styleEditText"
                android:layout_width="360dp"
                android:layout_height="38dp"
                android:background="@drawable/button1"
                android:hint="Masukkan NIM"
                android:paddingLeft="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.197"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtNim"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/txtJenisKelamin"
                android:layout_width="277dp"
                android:layout_height="20dp"
                android:layout_marginTop="136dp"
                android:text="@string/jk"
                android:textColor="@color/colorText"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.097"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Spinner
                android:id="@+id/spinner"
                android:entries="@array/JenisKelamin"
                android:layout_width="360dp"
                android:layout_height="38dp"
                android:background="@drawable/button1"
                android:paddingLeft="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.197"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtJenisKelamin"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/txtNoTelp"
                android:layout_width="247dp"
                android:layout_height="20dp"
                android:layout_marginTop="4dp"
                android:text="@string/no"
                android:textColor="@color/colorText"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.082"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/spinner" />

            <EditText
                android:id="@+id/editNO"
                style="@style/styleEditText"
                android:layout_width="360dp"
                android:layout_height="38dp"
                android:background="@drawable/button1"
                android:hint="Masukkan No.Telp / HP"
                android:paddingLeft="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.197"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtNoTelp"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/txtEmail"
                android:layout_width="261dp"
                android:layout_height="18dp"
                android:layout_marginTop="260dp"
                android:text="@string/emailA"
                android:textColor="@color/colorText"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.088"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/editEmail"
                style="@style/styleEditText"
                android:layout_width="360dp"
                android:layout_height="38dp"
                android:background="@drawable/button1"
                android:hint="Masukkan Email"
                android:paddingLeft="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.197"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtEmail"
                tools:ignore="MissingConstraints" />

            <!--android:onClick="clicked"-->

            <TextView
                android:id="@+id/txtSosmed"
                android:layout_width="285dp"
                android:layout_height="19dp"
                android:layout_marginTop="316dp"
                android:text="@string/sosmed"
                android:textColor="@color/colorText"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.115"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/editSosmed"
                style="@style/styleEditText"
                android:layout_width="360dp"
                android:layout_height="38dp"
                android:background="@drawable/button1"
                android:hint="Sosial Media"
                android:paddingLeft="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.197"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtSosmed"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/alamat"
                android:layout_width="269dp"
                android:layout_height="19dp"
                android:layout_marginTop="372dp"
                android:text="@string/alamatA"
                android:textColor="@color/colorText"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.104"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints" />

            <EditText
                android:id="@+id/editAlamat"
                style="@style/styleEditText"
                android:layout_width="360dp"
                android:layout_height="38dp"
                android:background="@drawable/button1"
                android:hint="Masukkan Alamat Lengkap"
                android:paddingLeft="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.197"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/alamat" />

            <TextView
                android:id="@+id/txtKeahlian"
                android:layout_width="269dp"
                android:layout_height="20dp"
                android:layout_marginTop="428dp"
                android:text="Keahlian"
                android:textColor="@color/colorText"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.104"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Spinner
                android:id="@+id/spinnerKeahlian"
                style="@style/styleEditText"
                android:entries="@array/keahlian"
                android:layout_width="360dp"
                android:layout_height="38dp"
                android:background="@drawable/button1"
                android:paddingLeft="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.197"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtKeahlian" />

            <TextView
                android:id="@+id/txtBidangMinat"
                android:layout_width="269dp"
                android:layout_height="20dp"
                android:layout_marginTop="487dp"
                android:text="Bidang Minat"
                android:textColor="@color/colorText"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.104"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="500dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.175"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <CheckBox
                    android:id="@+id/cbUI"
                    android:layout_width="83dp"
                    android:layout_height="29dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="14dp"
                    android:layout_marginLeft="14dp"
                    android:layout_marginEnd="344dp"
                    android:layout_marginRight="344dp"
                    android:layout_marginBottom="111dp"
                    android:layout_weight="1"
                    android:background="@drawable/button2"
                    android:text="UI/UX"
                    android:textAllCaps="false"
                    android:textColor="@color/kode"
                    android:textSize="15sp"
                    tools:layout_editor_absoluteX="30dp"
                    tools:layout_editor_absoluteY="533dp" />

                <CheckBox
                    android:id="@+id/cbFrontend"
                    android:layout_width="192dp"
                    android:layout_height="28dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="94dp"
                    android:layout_marginLeft="94dp"
                    android:layout_marginEnd="147dp"
                    android:layout_marginRight="147dp"
                    android:layout_marginBottom="111dp"
                    android:layout_weight="1"
                    android:background="@drawable/button2"
                    android:text="Frontend"
                    android:textAllCaps="false"
                    android:textColor="@color/kode"
                    android:textSize="15sp"
                    tools:layout_editor_absoluteX="30dp"
                    tools:layout_editor_absoluteY="533dp" />

                <CheckBox
                    android:id="@+id/cbDatabase"
                    android:layout_width="192dp"
                    android:layout_height="28dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="189dp"
                    android:layout_marginLeft="189dp"
                    android:layout_marginEnd="52dp"
                    android:layout_marginRight="52dp"
                    android:layout_marginBottom="112dp"
                    android:layout_weight="1"
                    android:background="@drawable/button2"
                    android:text="Database"
                    android:textAllCaps="false"
                    android:textColor="@color/kode"
                    android:textSize="15sp"
                    tools:layout_editor_absoluteX="30dp"
                    tools:layout_editor_absoluteY="533dp" />

                <CheckBox
                    android:id="@+id/cbWeb"
                    android:layout_width="192dp"
                    android:layout_height="28dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="14dp"
                    android:layout_marginLeft="14dp"
                    android:layout_marginEnd="227dp"
                    android:layout_marginRight="227dp"
                    android:layout_marginBottom="83dp"
                    android:layout_weight="1"
                    android:background="@drawable/button2"
                    android:text="Web Programming"
                    android:textAllCaps="false"
                    android:textColor="@color/kode"
                    android:textSize="15sp"
                    tools:layout_editor_absoluteX="30dp"
                    tools:layout_editor_absoluteY="533dp" />

                <CheckBox
                    android:id="@+id/cbAndroid"
                    android:layout_width="192dp"
                    android:layout_height="28dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="190dp"
                    android:layout_marginLeft="190dp"
                    android:layout_marginEnd="51dp"
                    android:layout_marginRight="51dp"
                    android:layout_marginBottom="83dp"
                    android:layout_weight="1"
                    android:background="@drawable/button2"
                    android:text="Android Developer"
                    android:textAllCaps="false"
                    android:textColor="@color/kode"
                    android:textSize="15sp"
                    tools:layout_editor_absoluteX="30dp"
                    tools:layout_editor_absoluteY="533dp" />

                <Button
                    android:id="@+id/btnKembali"
                    android:layout_width="161dp"
                    android:layout_height="36dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginEnd="270dp"
                    android:layout_marginRight="270dp"
                    android:layout_marginBottom="33dp"
                    android:layout_weight="1"
                    android:background="@drawable/button2"
                    android:text="Keluar"
                    android:textAllCaps="false"
                    android:textColor="@color/kode"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    tools:ignore="MissingConstraints" />

                <Button
                    android:id="@+id/btnSimpan"
                    android:layout_width="161dp"
                    android:layout_height="36dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="221dp"
                    android:layout_marginLeft="221dp"
                    android:layout_marginEnd="59dp"
                    android:layout_marginRight="59dp"
                    android:layout_marginBottom="32dp"
                    android:layout_weight="1"
                    android:background="@drawable/button2"
                    android:text="Simpan"
                    android:textAllCaps="false"
                    android:textColor="@color/kode"
                    android:textSize="10sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.803"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:ignore="MissingConstraints" />
            </RelativeLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
